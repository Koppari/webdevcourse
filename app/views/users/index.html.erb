<p id="notice"><%= notice %></p>

<h1>User list</h1>

<table>
  <thead>
  <tr>
    <th>Username</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <table>
    <tbody>
    <% @users.each do |user| %>
        <tr>
          <td><%= link_to user.username, user %></td>
        </tr>
    <% end %>
    </tbody>
  </table>

  <br>